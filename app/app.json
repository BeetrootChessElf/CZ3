{
  "expo": {
    "name": "CuppaZee",
    "slug": "cuppazee",
    "assetBundlePatterns": [
      "**/*"
    ],
    "orientation": "default",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "splash": {
      "backgroundColor": "#ddffe2",
      "image": "./assets/splash.png",
      "resizeMode": "contain"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "androidNavigationBar": {
      "backgroundColor": "#ddffe2",
      "barStyle": "dark-content"
    },
    "androidStatusBar": {
      "backgroundColor": "#ddffe2",
      "barStyle": "dark-content"
    },
    "plugins": [
      "expo-dev-client",
      "expo-splash-screen",
      "expo-community-flipper",
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsImpl": "mapbox-gl",
          "RNMapboxMapsDownloadToken": "pk.eyJ1Ijoic29oY2FoIiwiYSI6ImNqeWVqcm8wdTAxc2MzaXFpa282Yzd2aHEifQ.afYbt2sVMZ-kbwdx5_PekQ"
        }
      ]
    ]
  }
}
