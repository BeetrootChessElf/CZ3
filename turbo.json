{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".prisma/**",
        ".next/**",
        "lib/**"
      ]
    },
    "dev": {
      "dependsOn": [
        "^build"
      ],
      "cache": false
    },

    "api:dev": {
      "dependsOn": [
        "@cz3/api#dev"
      ]
    },
    "api:build": {
      "dependsOn": [
        "@cz3/api#build"
      ]
    },

    "lint": {
      "outputs": []
    },
    "lint:fix": {
      "outputs": []
    }
  }
}
